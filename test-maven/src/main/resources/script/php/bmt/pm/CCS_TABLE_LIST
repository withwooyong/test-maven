    "BTN_URI_CNTS" VARCHAR2(200 BYTE), 
    "LINK_ID" VARCHAR2(10 BYTE), 
    "CHNL_IMG_NM" VARCHAR2(10 BYTE), 
    "VAS_PATH_NM" VARCHAR2(200 BYTE), 
    "VAS_SVC_ID" VARCHAR2(200 BYTE), 
    "PBC_CNTS" VARCHAR2(258 BYTE), 
     CONSTRAINT "PK_IEIF_ABSTIMETRI_MST" PRIMARY KEY ("ID_SVC", "BTN_FR_DT")
