    "ID_INSERT" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
    "DT_UPDATE" DATE NOT NULL ENABLE, 
    "ID_UPDATE" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
     CONSTRAINT "PK_WEPG_SVR_IDX" PRIMARY KEY ("WEPG_SVR_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
